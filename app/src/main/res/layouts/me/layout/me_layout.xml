<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/app_background_color">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/me_home_header_cl"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_width="match_parent"
                android:background="@drawable/shape_fill_red_bg_me_head_drawable"
                android:layout_height="wrap_content">

            <ImageView
                    android:id="@+id/me_home_setting_iv"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="40dp"
                    android:layout_marginRight="12dp"
                    android:background="?android:attr/selectableItemBackground"
                    android:contentDescription="@null"
                    android:src="@drawable/ic_me_setting"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>

            <com.ling.kotlin.widget.CircleImageView
                    android:id="@+id/me_home_icon_iv"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginLeft="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_setting_iv"
                    android:background="@drawable/stroke_ripple_nomask_red_selector_drawable"
                    android:contentDescription="@null"
                    android:src="@mipmap/default_icon"/>
            <TextView
                    android:id="@+id/me_home_username_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    app:layout_constraintStart_toEndOf="@+id/me_home_icon_iv"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_setting_iv"
                    android:text="Ling36"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="8dp"
                    android:textColor="@color/white"
                    android:textSize="20sp"/>

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/me_home_level_cl"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/stroke_me_level_red_drawable"
                    android:paddingLeft="8dp"
                    android:paddingRight="8dp"
                    android:paddingBottom="6dp"
                    android:paddingTop="6dp"
                    app:layout_constraintStart_toEndOf="@+id/me_home_icon_iv"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_username_tv">
                <TextView
                        android:id="@+id/me_home_user_level_title_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:text="至尊赌神"
                        android:textSize="15sp"
                        android:textColor="@color/white"
                        app:layout_constraintBottom_toBottomOf="parent"/>
                <TextView
                        android:id="@+id/me_home_user_level_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:textSize="12sp"
                        android:background="@drawable/ic_me_vip_one"
                        android:textColor="@color/white"
                        android:paddingLeft="20dp"
                        android:gravity="center|left"
                        android:layout_marginStart="4dp"
                        android:layout_marginLeft="4dp"
                        app:layout_constraintStart_toEndOf="@+id/me_home_user_level_title_tv"
                        android:text="VIP16"
                        android:textStyle="italic|bold"
                        android:paddingRight="3dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <View
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_icon_iv"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
                android:id="@+id/me_home_wallet_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:padding="15dp"
                android:layout_marginTop="10dp"
                android:drawableLeft="@drawable/ic_me_wallet"
                android:drawablePadding="8dp"
                android:gravity="center_vertical"
                android:drawableRight="@drawable/ic_next"
                android:background="@drawable/stroke_ripple_mask_white_selector_drawable"
                app:layout_constraintTop_toBottomOf="@+id/me_home_header_cl"
                app:layout_constraintLeft_toLeftOf="parent"
                android:text="@string/me_wallet_title_hint"
                android:textColor="@color/app_text_main_another_normal_color"
                android:textSize="15sp"/>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/me_home_history_cl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                android:padding="14dp"
                app:layout_constraintTop_toBottomOf="@+id/me_home_wallet_tv"
                app:layout_constraintLeft_toLeftOf="parent"
                android:orientation="vertical">

            <TextView
                    android:id="@+id/me_home_history_wait_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="?android:attr/selectableItemBackground"
                    android:drawableTop="@drawable/ic_me_wait"
                    android:drawablePadding="10dp"
                    android:layout_marginLeft="35dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:gravity="center"
                    android:text="@string/me_history_wait_hint"
                    android:textColor="@color/app_text_main_another_normal_color"/>


            <View
                    android:layout_width="0.5dp"
                    android:layout_height="38dp"
                    app:layout_constraintStart_toEndOf="@+id/me_home_history_wait_tv"
                    app:layout_constraintEnd_toStartOf="@+id/me_home_history_winning_tv"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/app_e1e1e2_color"/>
            <TextView
                    android:id="@+id/me_home_history_winning_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:background="?android:attr/selectableItemBackground"
                    android:drawableTop="@drawable/ic_me_win"
                    android:drawablePadding="10dp"
                    android:gravity="center"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:text="@string/me_history_win_hint"
                    android:textColor="@color/app_text_main_another_normal_color"/>

            <View
                    android:layout_width="0.5dp"
                    android:layout_height="38dp"
                    app:layout_constraintStart_toEndOf="@+id/me_home_history_winning_tv"
                    app:layout_constraintEnd_toStartOf="@+id/me_home_history_not_winning_tv"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:background="@color/app_e1e1e2_color"/>
            <TextView
                    android:id="@+id/me_home_history_not_winning_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1.0"
                    android:background="?android:attr/selectableItemBackground"
                    android:drawableTop="@drawable/ic_me_nowin"
                    android:drawablePadding="10dp"
                    android:gravity="center"
                    android:layout_marginRight="35dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:text="@string/me_history_nowin_hint"
                    android:textColor="@color/app_text_main_another_normal_color"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/me_home_user_center_layout_cl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                android:background="@color/white"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/me_home_history_cl">
            <LinearLayout
                    android:id="@+id/me_home_user_center_cl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="10dp"
                    android:orientation="horizontal"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_account_center_cl"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_weight="1.0"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_center_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_me_center"/>
                    <TextView
                            android:id="@+id/me_home_user_center_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="账号管理"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_center_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_center_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_center_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_center_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="管理账号信息"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_level_cl"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_weight="1.0"
                        android:layout_height="wrap_content">

                    <ImageView
                            android:id="@+id/me_home_user_level_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:src="@drawable/ic_me_level"/>
                    <TextView
                            android:id="@+id/me_home_user_level_info_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="等级查询"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_level_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_level_info_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_level_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_level_info_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="等级奖励信息查询"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
            <LinearLayout
                    android:id="@+id/me_home_user_message_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_user_center_cl"
                    app:layout_constraintLeft_toLeftOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_message_cl"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_message_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_me_message"/>
                    <TextView
                            android:id="@+id/me_home_user_message_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="消息中心"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_message_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_message_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_message_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_message_tv"
                            android:textColor="@color/app_lottery_sliding_menu_text_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="最新站内活动消息"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_server_cl"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_server_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:src="@drawable/ic_me_server"/>
                    <TextView
                            android:id="@+id/me_home_user_server_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="客服中心"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_server_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_server_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_server_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_server_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="客户问题在线解答"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
            <LinearLayout
                    android:id="@+id/me_home_user_night_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_user_message_ll"
                    app:layout_constraintLeft_toLeftOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_night_cl"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_night_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_me_night"/>
                    <TextView
                            android:id="@+id/me_home_user_night_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="夜间模式"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_night_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_night_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_night_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_night_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="点击切换白天/夜间模式"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_version_cl"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_version_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:src="@drawable/ic_me_download"/>
                    <TextView
                            android:id="@+id/me_home_user_version_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="检测版本"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_version_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_version_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_version_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_version_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="当前版本V5.2.0"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
            <LinearLayout
                    android:id="@+id/me_home_user_setting_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingTop="15dp"
                    android:paddingBottom="15dp"
                    android:paddingLeft="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/me_home_user_night_ll"
                    app:layout_constraintLeft_toLeftOf="parent">
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_clear_cl"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_clear_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginLeft="5dp"
                            android:layout_marginStart="5dp"
                            android:src="@drawable/ic_me_clear_cache"/>
                    <TextView
                            android:id="@+id/me_home_user_clear_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="清除缓存"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_clear_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_clear_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_clear_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_clear_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="点击清除应用缓存"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/me_home_user_about_cl"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintTop_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_weight="1.0"
                        android:background="?android:attr/selectableItemBackground"
                        android:layout_height="wrap_content">
                    <ImageView
                            android:id="@+id/me_home_user_about_iv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:src="@drawable/ic_me_about"/>
                    <TextView
                            android:id="@+id/me_home_user_about_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textColor="@color/app_text_main_normal_color"
                            android:text="关于我们"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:textSize="16sp"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_about_iv"
                            app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                            android:id="@+id/me_home_user_about_hint_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:layout_constraintStart_toEndOf="@+id/me_home_user_about_iv"
                            app:layout_constraintTop_toBottomOf="@id/me_home_user_about_tv"
                            android:textColor="@color/app_text_three_normal_color"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2dp"
                            android:textSize="12sp"
                            android:text="关于我们详细信息"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginTop="15dp"
                app:layout_constraintTop_toBottomOf="@+id/me_home_user_center_layout_cl"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>



